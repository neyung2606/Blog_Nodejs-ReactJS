{
  "compilerOptions": {
    "baseUrl": "./",
    "experimentalDecorators": true,
    "jsx": "react",
    "paths": {
      "@components": ["src/components"],
      "@assets/*": ["src/assets/*"],
      "@pages/*": ["src/pages/*"],
      "@utils/*": ["src/utils/*"],
      "@stores": ["src/stores"],
      "@routes": ["src/routes"],
      "@apis/*": ["src/apis/*"],
      "@hooks/*": ["src/hooks/*"]
    }
  },
  "exclude": ["node_modules"]
}
